{
  "name": "react-trainingreport",
  "version": "2.0.13",
  "main": "dist/index.js",
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.1.1",
    "@testing-library/user-event": "^13.5.0",
    "@types/pdfmake": "^0.2.1",
    "@types/react": "^17.0.45",
    "@types/styled-components": "^5.1.25",
    "node-sass": "^4.14.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "^5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "set PORT=3002 && react-scripts start",
    "build": "rmdir /S /Q dist && tsc && npm run copy",
    "copy": "copy src\\types.d.ts dist\\types.d.ts && copy src\\lib\\*.js dist\\lib\\*.js",
    "major": "npm version major && npm run build && npm publish && git push origin main",
    "minor": "npm version minor && npm run build && npm publish && git push origin main",
    "patch": "npm version patch && npm run build && npm publish && git push origin main"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "author": "HelloJeong",
  "repository": {
    "type": "git",
    "url": "https://github.com/bnri/react-trainingreport"
  },
  "peerDependencies": {
    "react": ">= 17",
    "react-dom": ">= 17"
  },
  "engines": {
    "node": ">= 14"
  },
  "dependencies": {
    "chart.js": "^2.9.4",
    "chartjs-plugin-datalabels": "^0.7.0",
    "chartjs-plugin-doughnutlabel": "^2.0.3",
    "dayjs": "^1.11.5",
    "pdfmake": "^0.2.5",
    "react-chartjs-2": "^2.11.1",
    "styled-components": "^5.3.5",
    "typescript": "^4.7.2"
  }
}
